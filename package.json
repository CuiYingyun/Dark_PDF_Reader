{
  "name": "dark-pdf-reader-extension",
  "version": "0.1",
  "private": true,
  "scripts": {
    "sync:pdfjs": "bash ./scripts/sync-pdfjs-assets.sh",
    "test:arxiv": "node ./scripts/test-arxiv-playwright.mjs",
    "test:auto": "node ./scripts/test-auto-takeover-playwright.mjs",
    "test:resolver": "node ./scripts/test-resolver.mjs",
    "test:menu": "node ./scripts/test-context-menu-config.mjs",
    "test:edge:auto": "PDF_TEST_BROWSER=edge node ./scripts/test-auto-takeover-playwright.mjs",
    "test:edge:arxiv": "PDF_TEST_BROWSER=edge node ./scripts/test-arxiv-playwright.mjs",
    "test:edge:list": "PDF_TEST_BROWSER=edge node ./scripts/test-edge-arxiv-list-click-playwright.mjs",
    "test:edge:outline": "node ./scripts/test-edge-outline-behavior-playwright.mjs"
  },
  "keywords": [
    "chrome-extension",
    "pdf",
    "dark-mode"
  ],
  "license": "ISC",
  "description": "Chrome extension for local/web PDF dark inversion reading.",
  "dependencies": {
    "pdfjs-dist": "^3.11.174"
  },
  "devDependencies": {
    "playwright": "^1.58.2"
  }
}
